<h1>Hello World</h1>
<div data-controller="select"></div>
    <%= yield %>
	<% if user_signed_in? %>
		<div> Welcome <%= current_user.email %> </div>
  		<%= button_to 'Log Out', destroy_user_session_path, method: :delete , class: "btn btn-outline-danger"%>
		<%= link_to 'My links', links_url, class: "btn btn-outline-success" %>
	<% else %>
  		<%= button_to 'Sign Up', new_user_registration_path,method: :get  %>
  		<%= button_to 'Sign In', new_user_session_path %>
	<% end %>

<div id="<%= dom_id link %>">
  <td>
    <%= link.public_url %>
  </td>

  <td>
    <%= link_to "#{link.slug}", short_link_path(link.slug)%>
  </td>
  
  <td>
    <%= getSubclassTypeName(link.type) %>
  </td>

  <div class="link">
    <td>
      <% if link.type == "TemporaryLink" %>
          <%= link.expiration_date %>
      <% end %>
    </td>
  </div>

  <td>
    <%= link.active %>
  </td>
  
  <td>
    <%= link_to 'Visits', visits_path(link_id: link.id) %>
  </td>

  <td>
    <%= link_to "Edit", edit_link_path(link) %> 
  </td>
</div>

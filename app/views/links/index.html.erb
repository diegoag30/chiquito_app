<p style="color: green"><%= notice %></p>

<h1>Links</h1>

<div id="links">
  <% @links.each do |link| %>
    <div class="link">
    <% if link.type == "private" %>
      <%= render partial: 'private_link', locals: { link: link } %>
    <% elsif link.type == "temporary"%>
      <%= render partial: 'temporary_link', locals: { link: link } %>
    <%else%>
      <%= render partial: 'link', locals: { link: link } %>
    <% end %>
    </div>
  <% end %>
</div>

<%= link_to "New link", new_link_path %>
